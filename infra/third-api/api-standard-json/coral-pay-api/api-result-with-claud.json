{
  "baseUrl": "https://coralpay.com/api",
  "apiProtocol": "HTTPS",
  "isEncryptionUsed": "true",
  "encryptionAlgorithm": "PGP",
  "numberOfApis": "3",
  "apiList": [
    {
      "url": "/name-enquiry",
      "method": "POST",
      "headers": {
        "Content-Type": "text/plain",
        "SecretKey": "{{provided_by_coralpay}}"
      },
      "requestParams": [
        {
          "name": "sessionId",
          "description": "Unique 30-digit session ID",
          "dateType": "string",
          "isRequired": "true",
          "defaultValue": "",
          "example": "000966720915078958283981330700",
          "remark": "Must be 30 digits: 6-digit institution ID + 12-digit datetime + 12-digit unique identifier"
        },
        {
          "name": "destinationInstitutionId",
          "description": "Unique ID of destination institution",
          "dateType": "string",
          "isRequired": "true",
          "defaultValue": "",
          "example": "000703",
          "remark": "Institution code from provided list"
        },
        {
          "name": "accountId",
          "description": "Unique account number",
          "dateType": "string",
          "isRequired": "true",
          "defaultValue": "",
          "example": "1000211487",
          "remark": "Account number to verify"
        }
      ],
      "responseResults": [
        {
          "name": "sessionId",
          "description": "Unique session ID",
          "dateType": "string",
          "isRequired": "true",
          "defaultValue": "",
          "example": "000966720915078958283981330700",
          "remark": "Matches request sessionId"
        },
        {
          "name": "accountName",
          "description": "Name of account holder",
          "dateType": "string",
          "isRequired": "true",
          "defaultValue": "",
          "example": "Ify Obi",
          "remark": "Full name of account holder"
        },
        {
          "name": "responseCode",
          "description": "Transaction response code",
          "dateType": "string",
          "isRequired": "true",
          "defaultValue": "",
          "example": "00",
          "remark": "00 indicates successful transaction"
        }
      ]
    },
    {
      "url": "/post-credit",
      "method": "POST",
      "headers": {
        "Content-Type": "text/plain",
        "SecretKey": "{{provided_by_coralpay}}"
      },
      "requestParams": [
        {
          "name": "sessionId",
          "description": "Unique 30-digit session ID",
          "dateType": "string",
          "isRequired": "true",
          "defaultValue": "",
          "example": "000966444324566658283981389895",
          "remark": "Must be 30 digits: 6-digit institution ID + 12-digit datetime + 12-digit unique identifier"
        },
        {
          "name": "paymentRef",
          "description": "Unique payment reference number",
          "dateType": "string",
          "isRequired": "true",
          "defaultValue": "",
          "example": "2023012123345211290009765",
          "remark": "Unique reference generated by source institution"
        },
        {
          "name": "amount",
          "description": "Transaction amount",
          "dateType": "decimal",
          "isRequired": "true",
          "defaultValue": "",
          "example": "5000.00",
          "remark": "Amount must be greater than 0, max 16 digits"
        }
      ],
      "responseResults": [
        {
          "name": "sessionId",
          "description": "Unique session ID",
          "dateType": "string",
          "isRequired": "true",
          "defaultValue": "",
          "example": "000966444324566658283981389895",
          "remark": "Matches request sessionId"
        },
        {
          "name": "responseCode",
          "description": "Transaction response code",
          "dateType": "string",
          "isRequired": "true",
          "defaultValue": "",
          "example": "00",
          "remark": "00 indicates successful transaction"
        },
        {
          "name": "transactionDate",
          "description": "Date and time of transaction",
          "dateType": "datetime",
          "isRequired": "true",
          "defaultValue": "",
          "example": "2023-03-23 10:28:25.007",
          "remark": "Timestamp of transaction completion"
        }
      ]
    },
    {
      "url": "/transaction-query",
      "method": "POST",
      "headers": {
        "Content-Type": "text/plain",
        "SecretKey": "{{provided_by_coralpay}}"
      },
      "requestParams": [
        {
          "name": "sessionId",
          "description": "Unique 30-digit session ID",
          "dateType": "string",
          "isRequired": "true",
          "defaultValue": "",
          "example": "000966367432476535828398145678",
          "remark": "Must be 30 digits: 6-digit institution ID + 12-digit datetime + 12-digit unique identifier"
        }
      ],
      "responseResults": [
        {
          "name": "sessionId",
          "description": "Unique session ID",
          "dateType": "string",
          "isRequired": "true",
          "defaultValue": "",
          "example": "000966367432476535828398145678",
          "remark": "Matches request sessionId"
        },
        {
          "name": "paymentRef",
          "description": "Unique payment reference number",
          "dateType": "string",
          "isRequired": "true",
          "defaultValue": "",
          "example": "2023012123345211290009765",
          "remark": "Unique reference of queried transaction"
        },
        {
          "name": "responseCode",
          "description": "Transaction response code",
          "dateType": "string",
          "isRequired": "true",
          "defaultValue": "",
          "example": "00",
          "remark": "00 indicates successful transaction"
        }
      ]
    }
  ]
}